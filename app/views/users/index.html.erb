<h1>Listing Users</h1>
 <%= link_to 'New User', new_user_path %>
<table>
  <tr>
    <th>username</th>
    <th>email</th>
    <th>password</th>
    <th colspan="3"></th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.encrypted_password %></td>
      
    </tr>
  <% end %>
</table>